{
  "name": "egg-element-vue",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "dev": "vite --config scripts/config/vite.config.ts",
    "dev:page1": "vite serve src/module/page1 --config scripts/config/vite.config.ts ",
    "dev:page2": "vite serve src/module/page2 --config scripts/config/vite.config.ts",
    "build": "vite build --config scripts/config/vite.config.ts ",
    "preview": "vite preview",
    "lint": "eslint --config eslint.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/js": "^9.15.0",
    "@types/node": "^20.11.19",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-vue": "^5.2.0",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "conventional-changelog-cli": "^5.0.0",
    "cssnano": "^7.0.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.15.0",
    "eslint-plugin-vue": "^9.31.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.10",
    "postcss": "^8.4.49",
    "postcss-preset-env": "^10.1.1",
    "prettier": "^3.3.3",
    "stylelint": "^16.10.0",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-prettier": "^5.0.2",
    "terser": "^5.37.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^5.1.4",
    "vue-tsc": "^1.8.27"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint --config .commitlintrc.js -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint --config eslint.config.mjs"
    ],
    "*.{css,less,scss}": [
      "stylelint --config .stylelintrc.mjs"
    ],
    "*.{ts,tsx,js,json,html,yml,css,less,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": [
    "Android 4.1",
    "iOS 7.1",
    "Chrome > 31",
    "ff > 31",
    "ie >= 8",
    "last 2 versions"
  ],
  "dependencies": {
    "vue": "^3.5.13"
  }
}
